%p#notice= notice

%p
  %b Random key:
  = @ticket.random_key
%p
  %b Attendees:
  = @ticket.attendees
%p
  %b Paid:
  = !@ticket.unpaid
%p
  %b Ticket id:
  = @ticket.id

.btn-group
  = link_to 'Edit', edit_event_ticket_path(@event, @ticket), :class => 'btn'
  = link_to 'Back', event_path(@event), :class => 'btn'
